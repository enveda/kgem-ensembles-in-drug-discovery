{
  "type": "hpo",
  "metadata": {
    "title": "HPO Over 'Dataset' for TransE"
  },
  "pipeline": {
    "stopper": "early",
    "stopper_kwargs": {
      "frequency": 10,
      "patience": 3,
      "relative_delta": 0.002
    },
    "dataset_kwargs": {
      "create_inverse_triples": false
    },
    "model": "TransE",
    "model_kwargs": {},
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 5,
        "high": 8,
        "scale": "power_two"
      }
    },
    "loss": "BCEWithLogitsLoss",
    "regularizer": "NoRegularizer",
    "optimizer": "adam",
    "optimizer_kwargs": {
      "weight_decay": 0.0
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "lcwa",
    "training_loop_kwargs": {
      "automatic_memory_optimization": true
    },
    "training_kwargs": {
      "num_epochs": 300
    },
    "training_kwargs_ranges": {
      "label_smoothing": {
        "type": "float",
        "low": 0.001,
        "high": 1.0,
        "scale": "log"
      },
      "batch_size": {
        "type": "int",
        "low": 7,
        "high": 9,
        "scale": "power_two"
      }
    },
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": null
    }
  },
  "optuna": {
    "n_trials": 30,
    "timeout": 172800,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}
