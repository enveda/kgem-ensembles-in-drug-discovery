{
  "type": "hpo",
  "metadata": {
    "title": "HPO Over 'Dataset' for TransH"
  },
  "pipeline": {
    "stopper": "early",
    "stopper_kwargs": {
      "frequency": 50,
      "patience": 3,
      "relative_delta": 0.002
    },
    "dataset_kwargs": {
      "create_inverse_triples": false
    },
    "model": "TransH",
    "model_kwargs": {
      "embedding_dim": 256
    },
    "model_kwargs_ranges": {
      "embedding_dim": {
        "type": "int",
        "low": 7,
        "high": 9,
        "scale": "power_two"
      }
    },
    "loss": "NSSALoss",
    "optimizer": "adagrad",
    "optimizer_kwargs": {
      "weight_decay": 0.0
    },
    "optimizer_kwargs_ranges": {
      "lr": {
        "type": "float",
        "low": 0.0001,
        "high": 0.1,
        "scale": "log"
      }
    },
    "training_loop": "slcwa",
    "training_kwargs": {
      "num_epochs": 500
    },
    "training_kwargs_ranges": {
      "batch_size": {
        "type": "int",
        "low": 8,
        "high": 11,
        "scale": "power_two"
      }
    },
    "evaluator": "RankBasedEvaluator",
    "evaluator_kwargs": {
      "filtered": true
    },
    "evaluation_kwargs": {
      "batch_size": null
    }
  },
  "optuna": {
    "n_trials": 30,
    "timeout": 172800,
    "metric": "hits@10",
    "direction": "maximize",
    "sampler": "random",
    "pruner": "nop"
  }
}